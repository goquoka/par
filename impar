import java.util.Scanner;

 class Main { 
  public static void main(String[] args) {
    System.out.println("insira dois numeros");
  
    int a = 5;
    int b = 2;
    if ((a%2) == 0) {
      System.out.println("a é par");
    }else {
      System.out.println("a é impar"); 
    }
    if ((b%2) == 0) {
      System.out.println("b é par");
    } else {
      System.out.println("b é impar"); 
    }
  }
}
